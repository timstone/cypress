{
  "name": "cypress-project",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "cypress": "^4.9.0",
    "cypress-cucumber-preprocessor": "^2.5.1",
    "cypress-multi-reporters": "^1.4.0",
    "mocha": "^8.0.1",
    "mochawesome": "^6.1.1",
    "mochawesome-merge": "^4.1.0",
    "mochawesome-report-generator": "^5.1.0",
    "npm-run-all": "^4.1.5"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  },
  "scripts": {
    "cypress:open": "cypress open",
    "pretest": "npm run report:cleanup",
    "run": "cypress run --spec \"**/*.feature\"",
    "run:all": "cypress run --spec \"**/*.features\"",
    "test": "npm-run-all --serial run:all report:merge report:copyScreenshots report:generate --continue-on-error",
    "test:chrome": "cypress run --spec \"**/*.feature\" --browser chrome",
    "report:merge": "mochawesome-merge cypress/results/json/*.json > index.json",
    "report:copyScreenshots": "xcopy /s /i /e \".\\cypress\\screenshots\" \"public\\screenshots\"",
    "report:copyVideos": "xcopy /s /i /e \".\\cypress\\videos\" \"public\\videos\"",
    "report:generate": "marge index.json --reportDir public --assetsDir public/assets --reportPageTitle index.html",
    "report:cleanup": "del /q /s cypress\\results\\json\\* public\\*"
  },
  "keywords": [],
  "author": "Tim Stone",
  "license": "ISC"
}
